{
    "name": "alcedo-ui",
    "author": "Alcedo-UI Team",
    "version": "2.47.16",
    "description": "A set of React components.",
    "main": "./src/index.js",
    "scripts": {
        "compile:propType": "rimraf examples/assets/propTypes && gulp propType",
        "compile:componentsIndex": "rimraf src/index.js && node scripts/generateComponentsIndex.js",
        "compile:componentsStyle": "rimraf src/components.scss && node scripts/generateComponentsStyle.js",
        "compile": "npm run compile:propType && npm run compile:componentsIndex && npm run compile:componentsStyle",
        "pre-publish": "npm run compile && rimraf dist && cross-env NODE_ENV=production gulp copy",
        "do-publish": "npm run pre-publish && cd dist && npm publish",
        "dev": "npm run compile && cross-env NODE_ENV=development node build/dev/dev-server.js",
        "clean": "rimraf docs",
        "dll": "cross-env NODE_ENV=production node build/dll/dll.js",
        "package": "cross-env NODE_ENV=production node build/prod/prod.js",
        "build": "npm run clean && npm run compile && npm run dll && npm run package",
        "demo": "cross-env NODE_ENV=production node build/demo/server.js",
        "start": "npm run build && npm run demo",
        "zip": "node build/zip/zip.js"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/alcedo-ui/alcedo-ui.git"
    },
    "keywords": [
        "react",
        "react-component",
        "ui"
    ],
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/alcedo-ui/alcedo-ui/issues"
    },
    "homepage": "https://alcedo-ui.github.io/alcedo-ui/",
