{
    "name": "alcedo-ui",
    "author": "Alcedo-UI Team",
    "version": "2.22.6",
    "description": "A set of React components.",
    "main": "./src/index.js",
    "scripts": {
        "compile:propType": "rimraf examples/assets/propTypes && gulp propType",
        "compile:componentsIndex": "rimraf src/index.js && node scripts/generateComponentsIndex.js",
        "compile:componentsStyle": "rimraf src/components.scss && node scripts/generateComponentsStyle.js",
        "compile": "npm run compile:propType && npm run compile:componentsIndex && npm run compile:componentsStyle",
        "pre-publish": "rimraf dist && npm run compile && gulp copy",
        "dev": "npm run compile && cross-env NODE_ENV=development node build/dev/dev-server.js",
        "clean": "rimraf docs",
        "dll": "cross-env NODE_ENV=production node build/dll/dll.js",
        "package": "cross-env NODE_ENV=production node build/prod/prod.js",
        "build": "npm run clean && npm run compile && npm run dll && npm run package",
        "demo": "cross-env NODE_ENV=production node build/demo/demo-server.js",
        "start": "npm run build && npm run demo"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/alcedo-ui/alcedo-ui.git"
    },
    "keywords": [
        "react",
        "react-component",
        "ui"
    ],
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/alcedo-ui/alcedo-ui/issues"
    },
    "homepage": "https://alcedo-ui.github.io/alcedo-ui/",
    "dependencies": {
        "babel-polyfill": "^6.26.0",
        "classnames": "^2.2.5",
        "dom-helpers": "^3.3.1",
        "history": "^4.7.2",
        "lodash": "^4.17.5",
        "moment": "^2.22.1",
        "prop-types": "^15.6.1",
        "react": "^16.3.2",
        "react-beautiful-dnd": "^7.1.0",
        "react-dnd": "^2.6.0",
        "react-dnd-html5-backend": "^2.6.0",
        "react-dnd-scrollzone": "^4.0.0",
        "react-dom": "^16.3.2",
        "react-redux": "^5.0.7",
        "react-router": "^4.2.0",
        "react-router-config": "^1.0.0-beta.4",
        "react-router-dom": "^4.2.2",
        "react-router-redux": "^5.0.0-alpha.6",
        "react-tap-event-plugin": "^3.0.2",
        "react-transition-group": "^2.3.1",
        "redux": "^4.0.0",
        "redux-thunk": "^2.2.0"
    },
    "devDependencies": {
        "assets-webpack-plugin": "^3.5.1",
        "babel-core": "^6.26.0",
        "babel-loader": "^7.1.4",
        "babel-plugin-transform-decorators-legacy": "^1.3.4",
        "babel-plugin-transform-react-remove-prop-types": "^0.4.13",
        "babel-plugin-transform-runtime": "^6.23.0",
        "babel-preset-env": "^1.6.1",
        "babel-preset-react": "^6.24.1",
        "babel-preset-react-hmre": "^1.1.1",
        "babel-preset-stage-0": "^6.24.1",
        "babel-runtime": "^6.26.0",
        "browserify": "^16.2.0",
        "chalk": "^2.4.0",
        "compression": "^1.7.2",
        "compression-webpack-plugin": "^1.1.11",
        "connect-history-api-fallback": "^1.5.0",
        "copy-webpack-plugin": "^4.5.1",
        "cross-env": "^5.1.4",
        "css-loader": "^0.28.11",
        "eventsource-polyfill": "^0.9.6",
        "express": "^4.16.3",
        "file-loader": "^1.1.11",
        "friendly-errors-webpack-plugin": "^1.7.0",
        "gulp": "^3.9.1",
        "gulp-babel": "^7.0.1",
        "gulp-rename": "^1.2.2",
        "gulp-sequence": "^1.0.0",
        "gulp-util": "^3.0.8",
        "html-loader": "^0.5.5",
        "html-webpack-include-assets-plugin": "^1.0.4",
        "html-webpack-plugin": "^3.2.0",
        "node-sass": "^4.8.3",
        "opn": "^5.3.0",
        "ora": "^2.0.0",
        "path": "^0.12.7",
        "postcss-loader": "^2.1.4",
        "react-hot-loader": "^4.1.1",
        "rimraf": "^2.6.2",
        "sass-loader": "^7.0.1",
        "style-loader": "^0.21.0",
        "through2": "^2.0.3",
        "uglifyjs-webpack-plugin": "^1.2.5",
        "url-loader": "^1.0.1",
        "webpack": "^4.6.0",
        "webpack-dev-middleware": "^3.1.2",
        "webpack-hot-middleware": "^2.22.1",
        "webpack-merge": "^4.1.2"
    }
}
