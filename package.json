{
    "name": "alcedo-ui",
    "author": "Alcedo-UI Team",
    "version": "2.0.9",
    "description": "A set of React components.",
    "main": "./src/index.js",
    "scripts": {
        "compile:propType": "rimraf examples/assets/propTypes && gulp propType",
        "compile:componentsIndex": "rimraf src/index.js && node scripts/generateComponentsIndex.js",
        "compile:componentsStyle": "rimraf src/components.scss && node scripts/generateComponentsStyle.js",
        "compile": "npm run compile:propType && npm run compile:componentsIndex && npm run compile:componentsStyle",
        "publish": "rimraf dist && npm run compile && gulp copy",
        "dev": "npm run compile && node build/dev/dev-server.js",
        "build": "rimraf docs && npm run compile && node build/prod/prod.js",
        "run": "npm run build && node build/demo/demo-server.js"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/alcedo-ui/alcedo-ui.git"
    },
    "keywords": [
        "react",
        "react-component",
        "ui"
    ],
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/alcedo-ui/alcedo-ui/issues"
    },
    "homepage": "https://alcedo-ui.github.io/alcedo-ui/",
    "dependencies": {
        "history": "^4.7.2",
        "lodash": "^4.17.4",
        "moment": "^2.18.1",
        "prop-types": "^15.6.0",
        "react": "^16.0.0",
        "react-addons-transition-group": "^15.6.2",
        "react-dnd": "^2.5.4",
        "react-dnd-html5-backend": "^2.5.4",
        "react-dnd-scrollzone": "^4.0.0",
        "react-dom": "^16.0.0",
        "react-redux": "^5.0.6",
        "react-router": "^4.2.0",
        "react-router-config": "^1.0.0-beta.4",
        "react-router-dom": "^4.2.2",
        "react-router-redux": "^5.0.0-alpha.6",
        "react-tap-event-plugin": "^3.0.2",
        "redux": "^3.7.2",
        "redux-thunk": "^2.2.0"
    },
    "devDependencies": {
        "babel-core": "^6.26.0",
        "babel-loader": "^7.1.2",
        "babel-plugin-transform-decorators-legacy": "^1.3.4",
        "babel-plugin-transform-runtime": "^6.23.0",
        "babel-preset-env": "^1.6.0",
        "babel-preset-react": "^6.24.1",
        "babel-preset-react-hmre": "^1.1.1",
        "babel-preset-stage-0": "^6.24.1",
        "babel-runtime": "^6.26.0",
        "browserify": "^14.4.0",
        "compression": "^1.7.1",
        "compression-webpack-plugin": "^1.0.1",
        "connect-history-api-fallback": "^1.3.0",
        "copy-webpack-plugin": "^4.1.1",
        "css-loader": "^0.28.7",
        "eventsource-polyfill": "^0.9.6",
        "express": "^4.16.1",
        "extract-text-webpack-plugin": "^3.0.1",
        "file-loader": "^1.1.5",
        "friendly-errors-webpack-plugin": "^1.6.1",
        "gulp": "^3.9.1",
        "gulp-babel": "^7.0.0",
        "gulp-rename": "^1.2.2",
        "gulp-sequence": "^0.4.6",
        "gulp-util": "^3.0.8",
        "html-loader": "^0.5.1",
        "html-webpack-plugin": "^2.30.1",
        "json-loader": "^0.5.7",
        "node-sass": "^4.5.3",
        "opn": "^5.1.0",
        "optimize-css-assets-webpack-plugin": "^3.2.0",
        "ora": "^1.3.0",
