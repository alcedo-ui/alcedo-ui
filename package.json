{
    "name": "alcedo-ui",
    "author": "Alcedo-UI Team",
    "version": "1.1.7",
    "description": "A set of React components.",
    "main": "./src/index.js",
    "scripts": {
        "build:propType": "rimraf examples/assets/propTypes && gulp propType",
        "build:index": "rimraf src/index.js && node scripts/generateComponentsIndex.js",
        "build:components": "rimraf dist && npm run build:index && gulp build",
        "watch:components": "npm run build:components && npm run build:propType && gulp watch",
        "dev:example": "node build/dev/dev-server.js",
        "build:example": "rimraf docs && npm run build:components && npm run build:propType && node build/prod/prod.js",
        "run:demo": "node build/demo/demo-server.js",
        "demo": "npm run build:example && npm run run:demo"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/alcedo-ui/alcedo-ui.git"
    },
    "keywords": [
        "react",
        "react-component",
        "ui"
    ],
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/alcedo-ui/alcedo-ui/issues"
    },
    "homepage": "https://alcedo-ui.github.io/alcedo-ui/",
    "dependencies": {
        "lodash": "^4.17.4",
        "moment": "^2.17.1",
        "prop-types": "^15.5.8",
        "react": "^15.5.4",
        "react-addons-transition-group": "^15.4.2",
        "react-dnd": "^2.4.0",
        "react-dnd-html5-backend": "^2.4.1",
        "react-dnd-scrollzone": "^4.0.0",
        "react-dom": "^15.4.2",
        "react-redux": "^5.0.3",
        "react-router": "^3.0.2",
        "react-router-redux": "^4.0.8",
        "react-tap-event-plugin": "^2.0.1",
        "redux": "^3.6.0",
        "redux-thunk": "^2.2.0"
    },
    "devDependencies": {
        "babel-core": "^6.23.1",
        "babel-loader": "^6.4.0",
        "babel-plugin-transform-decorators-legacy": "^1.3.4",
        "babel-plugin-transform-runtime": "^6.23.0",
        "babel-preset-es2015": "^6.22.0",
        "babel-preset-react": "^6.23.0",
        "babel-preset-react-hmre": "^1.1.1",
        "babel-preset-stage-0": "^6.22.0",
        "babel-runtime": "^6.23.0",
        "browserify": "^14.3.0",
        "compression": "^1.7.0",
        "compression-webpack-plugin": "^1.0.0",
        "connect-history-api-fallback": "^1.3.0",
