        "friendly-errors-webpack-plugin": "^1.6.1",
        "gulp": "^3.9.1",
        "gulp-babel": "^6.1.2",
        "gulp-rename": "^1.2.2",
        "gulp-sass": "^3.1.0",
        "gulp-sequence": "^0.4.6",
        "gulp-util": "^3.0.8",
        "html-loader": "^0.4.5",
        "html-webpack-plugin": "^2.28.0",
        "json-loader": "^0.5.4",
        "node-sass": "^4.5.3",
        "opn": "^4.0.2",
        "optimize-css-assets-webpack-plugin": "^1.3.1",
