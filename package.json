{
    "name": "alcedo-ui",
    "author": "Alcedo-UI Team",
    "version": "2.30.1",
    "description": "A set of React components.",
    "main": "./src/index.js",
    "scripts": {
        "compile:propType": "rimraf examples/assets/propTypes && gulp propType",
        "compile:componentsIndex": "rimraf src/index.js && node scripts/generateComponentsIndex.js",
        "compile:componentsStyle": "rimraf src/components.scss && node scripts/generateComponentsStyle.js",
        "compile": "npm run compile:propType && npm run compile:componentsIndex && npm run compile:componentsStyle",
        "pre-publish": "rimraf dist && npm run compile && gulp copy",
        "do-publish": "npm run pre-publish && cd dist && npm publish",
        "dev": "npm run compile && cross-env NODE_ENV=development node build/dev/dev-server.js",
        "clean": "rimraf docs",
        "dll": "cross-env NODE_ENV=production node build/dll/dll.js",
        "package": "cross-env NODE_ENV=production node build/prod/prod.js",
        "build": "npm run clean && npm run compile && npm run dll && npm run package",
        "demo": "cross-env NODE_ENV=production node build/demo/demo-server.js",
        "start": "npm run build && npm run demo"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/alcedo-ui/alcedo-ui.git"
    },
    "keywords": [
        "react",
        "react-component",
        "ui"
    ],
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/alcedo-ui/alcedo-ui/issues"
    },
    "homepage": "https://alcedo-ui.github.io/alcedo-ui/",
    "dependencies": {
        "babel-polyfill": "^6.26.0",
        "classnames": "^2.2.6",
        "dom-helpers": "^3.3.1",
        "history": "^4.7.2",
        "lodash": "^4.17.10",
        "moment": "^2.22.2",
        "prop-types": "^15.6.2",
        "react": "^16.4.2",
        "react-beautiful-dnd": "^9.0.1",
        "react-dnd": "^3.0.2",
        "react-dnd-html5-backend": "^3.0.2",
        "react-dnd-scrollzone": "^4.0.0",
        "react-dom": "^16.4.2",
        "react-redux": "^5.0.7",
        "react-router": "^4.3.1",
        "react-router-config": "^1.0.0-beta.4",
        "react-router-dom": "^4.3.1",
        "react-router-redux": "^5.0.0-alpha.6",
        "react-transition-group": "^2.4.0",
        "redux": "^4.0.0",
        "redux-thunk": "^2.3.0"
    },
    "devDependencies": {
        "assets-webpack-plugin": "^3.9.6",
        "autoprefixer": "^9.1.3",
        "babel-core": "^6.26.3",
        "babel-loader": "^7.1.5",
        "babel-plugin-syntax-dynamic-import": "^6.18.0",
        "babel-plugin-transform-decorators-legacy": "^1.3.5",
        "babel-plugin-transform-import-sync": "^1.0.1",
        "babel-plugin-transform-react-remove-prop-types": "^0.4.15",
        "babel-plugin-transform-runtime": "^6.23.0",
        "babel-preset-env": "^1.7.0",
        "babel-preset-react": "^6.24.1",
        "babel-preset-react-hmre": "^1.1.1",
        "babel-preset-stage-0": "^6.24.1",
        "babel-runtime": "^6.26.0",
        "browserify": "^16.2.2",
        "compression": "^1.7.3",
        "compression-webpack-plugin": "^1.1.11",
        "connect-history-api-fallback": "^1.5.0",
        "copy-webpack-plugin": "^4.5.2",
        "cross-env": "^5.2.0",
        "css-loader": "^1.0.0",
        "eventsource-polyfill": "^0.9.6",
        "express": "^4.16.3",
        "fast-sass-loader": "^1.4.5",
        "file-loader": "^2.0.0",
        "friendly-errors-webpack-plugin": "^1.7.0",
        "gulp": "^3.9.1",
        "gulp-babel": "^7.0.1",
        "gulp-rename": "^1.4.0",
        "gulp-sequence": "^1.0.0",
        "gulp-util": "^3.0.8",
        "happypack": "^5.0.0",
        "html-loader": "^0.5.5",
        "html-webpack-include-assets-plugin": "^1.0.4",
        "html-webpack-plugin": "^3.2.0",
        "node-sass": "^4.9.3",
        "opn": "^5.3.0",
        "path": "^0.12.7",
        "postcss-loader": "^3.0.0",
        "react-hot-loader": "^4.3.4",
        "rimraf": "^2.6.2",
        "sass-loader": "^7.1.0",
        "style-loader": "^0.22.1",
        "through2": "^2.0.3",
        "uglifyjs-webpack-plugin": "^1.3.0",
        "url-loader": "^1.1.1",
        "webpack": "^4.17.1",
        "webpack-dev-middleware": "^3.1.3",
        "webpack-hot-middleware": "^2.22.3",
        "webpack-merge": "^4.1.4"
    }
}
